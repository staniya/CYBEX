#: ../../API/constants.rst:2
msgid "Constants"
msgstr "Malang"

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType:1
#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.ChatType:1
#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.TargetType:1
#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.ChannelType:1
msgid "An enumeration."
msgstr "Penghitungan."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Audio:1
msgid "Audio message. Including music and voice message."
msgstr "Mesej audio. Termasuk muzik dan mesej suara."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.File:1
msgid "Messages sent as a file."
msgstr "Mesej dihantar sebagai fail."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Image:1
msgid "Image (picture) message"
msgstr "Mesej gambar (gambar)"

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Link:1
msgid "Message that is mainly one specific link, or a text message with one link preview."
msgstr "Mesej yang terutamanya satu pautan tertentu, atau mesej teks dengan satu pratonton pautan."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Location:1
msgid "Location message."
msgstr "Mesej lokasi."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Status:1
msgid "Status from a user in a chat, usually typing and uploading."
msgstr "Status dari pengguna dalam sembang, biasanya menaip dan memuat naik."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Sticker:1
msgid "Pictures sent with few text caption, usually a transparent background, and a limited number of options that is usually not from the user's photo gallery."
msgstr "Gambar yang dihantar dengan beberapa teks teks, biasanya latar belakang telus, dan beberapa pilihan terhad yang biasanya bukan dari galeri foto pengguna."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Text:1
msgid "Text message"
msgstr "Mesej teks"

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Unsupported:1
msgid "Any type of message that is not listed above. A text representation is required."
msgstr "Mana-mana jenis mesej yang tidak disenaraikan di atas. Perwakilan teks diperlukan."

#: ../../../ehforwarderbot/constants.py:docstring of ehforwarderbot.MsgType.Video:1
msgid "Video message"
msgstr ""

